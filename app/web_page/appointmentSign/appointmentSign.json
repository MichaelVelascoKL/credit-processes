      "value" : [
        "if( $data.signPromissoryNote ){",
        "\treturn {",
        "\t\t//map signPromissoryNote variable to expected task contract input",
        "\t\tsignPromissoryNoteInput: {",
        "\t\t\tdateTimePromissoryNote: $data.signPromissoryNote.dateTimePromissoryNote !== undefined ? $data.signPromissoryNote.dateTimePromissoryNote : null,",
        "\t\t\tnumberPromissoryNote: $data.signPromissoryNote.numberPromissoryNote !== undefined ? $data.signPromissoryNote.numberPromissoryNote : null,",
