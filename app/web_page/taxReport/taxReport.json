  "variables" : {
    "task" : {
      "type" : "url",
      "value" : [
        "../API/bpm/userTask/{{taskId}}"
      ],
      "exposed" : false
    },
    "submit_errors_list" : {
      "type" : "expression",
      "value" : [
        "if($data.formOutput && $data.formOutput._submitError && $data.formOutput._submitError.explanations){",
        "\tconst liElements = $data.formOutput._submitError.explanations",
        "\t\t.filter(cause => cause !== null)",
        "\t\t.map(cause => \"<li>\" + cause + \"</li>\")",
        "\t\t.join('');",
        "\tif(liElements){",
        "\t\treturn \"<ul>\" + liElements + \"</ul>\";",
        "\t}",
        "}"
      ],
      "exposed" : false
    },
    "formOutput" : {
      "type" : "expression",
      "value" : [
        "if( $data.taxStudy ){",
        "\treturn {",
        "\t\t//map taxStudy variable to expected task contract input",
        "\t\ttaxStudyInput: {",
        "\t\t\tentityTaxStudy: $data.taxStudy.entityTaxStudy !== undefined ? $data.taxStudy.entityTaxStudy : null,",
        "\t\t\tresultTaxStudy: $data.taxStatus == 'Al día' ? true : false,",
        "\t\t\tstatusTaxStudy: $data.taxStatus == '' ? null : $data.taxStatus == 'Al día' ? true : false,",
        "\t\t\treasonTaxStudy: $data.taxStudy.reasonTaxStudy !== undefined ? $data.taxStudy.reasonTaxStudy : null",
        "\t\t}",
        "\t}",
        "}"
      ],
      "exposed" : false
    },
    "context" : {
      "type" : "url",
      "value" : [
        "../API/bpm/userTask/{{taskId}}/context"
      ],
      "exposed" : false
    },
    "taxStudy" : {
      "type" : "url",
      "value" : [
        "../{{context.taxStudy_ref.link}}"
      ],
      "exposed" : false
    },
    "taskId" : {
      "type" : "urlparameter",
      "value" : [
        "id"
      ],
      "exposed" : false
    },
    "taxStatus" : {
      "type" : "constant",
      "value" : [
        "Atrasado"
      ],
      "exposed" : false
    },
    "taxStatusText" : {
      "type" : "expression",
      "value" : [
        "return $data.taxStatus === '' ? 'Devuelto' :",
        "    $data.taxStatus === 'Al día' ? 'Aprobado' : ",
        "    'Rechazado'"
      ],
      "exposed" : false
    }
