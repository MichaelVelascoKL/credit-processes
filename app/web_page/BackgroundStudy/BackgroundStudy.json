        "if($data.formOutput && $data.formOutput._submitError && $data.formOutput._submitError.explanations){",
        "\tconst liElements = $data.formOutput._submitError.explanations",
        "\t\t.filter(cause => cause !== null)",
        "\t\t.map(cause => \"<li>\" + cause + \"</li>\")",
        "\t\t.join('');",
        "\tif(liElements){",
        "\t\treturn \"<ul>\" + liElements + \"</ul>\";",
        "\t}",
        "}"
      ],
      "exposed" : false
    },
    "formOutput" : {
      "type" : "expression",
      "value" : [
        "if( $data.backgroundStudy ){",
        "\treturn {",
        "\t\t//map backgroundStudy variable to expected task contract input",
        "\t\tbackgroundStudyInput: {",
        "\t\t\tentityBackStudy: $data.backgroundStudy.entityBackStudy !== undefined ? $data.backgroundStudy.entityBackStudy : null,",
        "\t\t\tresultBackStudy: $data.yesNoStatus === \"Si\" ? true : false,",
        "\t\t\tstatusBackStudy: $data.yesNoStatus === \"Si\" ? false : true,",
        "\t\t\treasonBackStudy: $data.backgroundStudy.reasonBackStudy !== undefined ? $data.backgroundStudy.reasonBackStudy : null",
        "\t\t}",
        "\t}",
        "}"
      ],
      "exposed" : false
